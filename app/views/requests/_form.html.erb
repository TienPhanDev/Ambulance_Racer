<%= form_for @request do |f| %>
<%= render 'shared/request_error_messages' %>
<%= f.label :title %>
<%= f.text_field :title %>
<br><br>
<%= f.label :date %>
<%= f.date_field :date  %>
<br><br>
<%= f.label :injury %>
<%= f.text_field :injury %>
<br><br>
<%= f.label :damages %>
<%= f.number_field :damages %>
<br><br>
<%= f.label :description %>
<%= f.text_area :description %>
<br><br>
<%= f.label :police_report %>
<%= f.check_box :police_report, {}, 'true' %>
<br><br>
<%= f.hidden_field :client_id, value: session[:user_id] %>
<%= f.submit %>
<% end %>